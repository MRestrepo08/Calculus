import numpy as np
import sympy as sym
from sympy import *
x, y, l = symbols('x y l')
init_printing()
import math
#Max/min
f = x*y
g = 4*x**2+9*y**2-36
w = f-l*g
#Partial Derivatives
wx = diff(w,x)
wy = diff(w,y)
wl = diff(w,l)
#Critical Points
cp = solve([wx, wy, wl],[x, y, l])
cp
#Function to evaluate
def maxmin(w):
  pc = solve([wx, wy, wl], [x, y, l])
  for p in pc:
    a = w.subs({x:p[0], y:p[1], l:p[2]})
    print(p, a)
 maxmin(w)
